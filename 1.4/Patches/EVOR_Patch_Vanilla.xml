<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyDef[defName="Human"]/corePart/parts</xpath>
		<order>Append</order>
		<!--Note: These defs are applied in reverse order, so the backs will be below the body cavities-->
		<value>
			<li>
				<def>Tail</def>
				<customLabel>tail bone</customLabel>
				<coverage>0.002</coverage>
				<depth>Outside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
			<li>
				<def>Back</def>
				<customLabel>lower back</customLabel>
				<coverage>0.020</coverage>
				<depth>Outside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
			<li>
				<def>Back</def>
				<customLabel>upper back</customLabel>
				<coverage>0.020</coverage>
				<depth>Outside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
			<li>
				<def>BodyCavityA</def>
				<customLabel>right abdominal cavity</customLabel>
				<coverage>0.010</coverage>
				<depth>Inside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
			<li>
				<def>BodyCavityA</def>
				<customLabel>left abdominal cavity</customLabel>
				<coverage>0.010</coverage>
				<depth>Inside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
			<li>
				<def>BodyCavity2</def>
				<customLabel>right chest cavity</customLabel>
				<coverage>0.010</coverage>
				<depth>Inside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
			<li>
				<def>BodyCavity1</def>
				<customLabel>left chest cavity</customLabel>
				<coverage>0.010</coverage>
				<depth>Inside</depth>
				<groups>
					<li>Torso</li>
				</groups>
			</li>
		</value>
	</Operation>
	<Operation Class="EvolvedOrgansRedux.PatchOperationAddTwoArms">
		<xpath>Defs/BodyDef[defName="Human"]/corePart/parts</xpath>
		<order>Append</order>
		<value>
			<li>
				<def>LowerShoulder</def>
				<customLabel>right hypocondriac shoulder</customLabel>
				<woundAnchorTag>RightLowerShoulder</woundAnchorTag>
				<coverage>0.00</coverage>
				<groups>
					<li>Shoulders</li>
				</groups>
			</li>
			<li>
				<def>LowerShoulder</def>
				<customLabel>left hypocondriac shoulder</customLabel>
				<woundAnchorTag>LeftLowerShoulder</woundAnchorTag>
				<coverage>0.00</coverage>
				<groups>
					<li>Shoulders</li>
				</groups>
			</li>
		</value>
	</Operation>
	<Operation Class="EvolvedOrgansRedux.PatchOperationAddFourArms">
		<xpath>Defs/BodyDef[defName="Human"]/corePart/parts</xpath>
		<order>Append</order>
		<value>
			<li>
				<def>LowerShoulder</def>
				<customLabel>right lumbar shoulder</customLabel>
				<woundAnchorTag>RightLowerShoulder</woundAnchorTag>
				<coverage>0.00</coverage>
				<groups>
					<li>Shoulders</li>
				</groups>
			</li>
			<li>
				<def>LowerShoulder</def>
				<customLabel>left lumbar shoulder</customLabel>
				<woundAnchorTag>LeftLowerShoulder</woundAnchorTag>
				<coverage>0.00</coverage>
				<groups>
					<li>Shoulders</li>
				</groups>
			</li>
		</value>
	</Operation>
	<Operation Class="EvolvedOrgansRedux.PatchOperationAddSixArms">
		<xpath>Defs/BodyDef[defName="Human"]/corePart/parts</xpath>
		<order>Append</order>
		<value>
			<li>
				<def>LowerShoulder</def>
				<customLabel>right iliac shoulder</customLabel>
				<woundAnchorTag>RightLowerShoulder</woundAnchorTag>
				<coverage>0.00</coverage>
				<groups>
					<li>Shoulders</li>
				</groups>
			</li>
			<li>
				<def>LowerShoulder</def>
				<customLabel>left iliac shoulder</customLabel>
				<woundAnchorTag>LeftLowerShoulder</woundAnchorTag>
				<coverage>0.00</coverage>
				<groups>
					<li>Shoulders</li>
				</groups>
			</li>
		</value>
	</Operation>
</Patch>

